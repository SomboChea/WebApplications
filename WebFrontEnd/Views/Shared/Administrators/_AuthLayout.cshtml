<!doctype html>
<html class="no-js h-100" lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>@ViewData["title"] | Authentication</title>
    <meta name="description" content="Administrator">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    
    <partial name="~/Views/Shared/_MaterialProStyles.cshtml" />
    <link rel="stylesheet" href="~/css/blue.css" />
    <link href="~/css/mt-style.css" rel="stylesheet" />

</head>
<body>

    <!-- ============================================================== -->
    <!-- Preloader - style you can find in spinners.css -->
    <!-- ============================================================== -->
    <div class="preloader">
        <svg class="circular" viewBox="25 25 50 50">
            <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="2" stroke-miterlimit="10" />
        </svg>
    </div>

    <!-- ============================================================== -->
    <!-- Main wrapper - style you can find in pages.scss -->
    <!-- ============================================================== -->
    <section id="wrapper">
        <div class="login-register">
            <div class="login-box card">
                <div class="card-body">
                    @RenderBody()
                </div>
            </div>
        </div>
    </section>

     <partial name="~/Views/Shared/_MaterialProScripts.cshtml" />
</body>
</html>